{
	"name": "github-graphql",
	"description": "This application allows the user to merge their GitHub pull-requests from a simple to use interface.",
	"license": "MIT",
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"start:docker": "cross-var docker build -t $npm_package_name . && cross-var docker run --rm -d -e Environment=Development -p 3000:80 --name $npm_package_name $npm_package_name:latest",
		"generate": "nuxt generate",
		"lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . --fix",
		"test": "jest",
		"format": "prettier --write .",
		"updateSnapshot": "jest --updateSnapshot",
		"heroku-postbuild": "npm run build"
	},
	"dependencies": {
		"@nuxtjs/apollo": "4.0.1-rc.5",
