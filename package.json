{
	"name": "app-template",
	"description": "This portal is used to access all Power BI reports and dashboards across the organization",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"start:docker": "cross-var docker build -t $npm_package_name . && cross-var docker run --rm -d -e Environment=Development -p 3000:80 --name $npm_package_name $npm_package_name:latest",
		"generate": "nuxt generate",
		"lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . --fix",
		"test": "jest",
		"format": "prettier --write .",
		"updateSnapshot": "jest --updateSnapshot"
	},
	"dependencies": {
		"@nuxtjs/apollo": "4.0.1-rc.5",
		"@nuxtjs/auth-next": "5.0.0-1624817847.21691f1",
		"@nuxtjs/axios": "5.13.6",
		"graphql-tag": "2.12.5",
		"nuxt": "2.15.7",
		"nuxt-typed-vuex": "0.2.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.15.8",
		"@mdi/js": "5.9.55",
		"@nuxt/types": "2.15.7",
		"@nuxt/typescript-build": "2.1.0",
		"@nuxtjs/eslint-config": "6.0.1",
		"@nuxtjs/eslint-config-typescript": "6.0.1",
		"@nuxtjs/eslint-module": "3.0.2",
		"@nuxtjs/vuetify": "1.12.1",
		"@types/jest": "26.0.24",
		"@types/lodash": "4.14.171",
		"@typescript-eslint/typescript-estree": "4.33.0",
		"@vue/test-utils": "1.2.1",
		"babel-core": "7.0.0-bridge.0",
		"babel-jest": "27.0.6",
		"core-js": "3.15.2",
		"cross-var": "1.1.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-codeceptjs": "1.3.0",
		"eslint-plugin-nuxt": "2.0.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-vuetify": "1.1.0",
		"eslint-webpack-plugin": "3.0.1",
		"jest": "27.0.6",
		"jest-transform-graphql": "2.1.0",
		"prettier": "2.4.1",
		"sass": "~1.32.13",
		"ts-jest": "27.0.3",
		"ts-loader": "9.0.1",
		"ts-node": "10.1.0",
		"typescript": "4.3.5",
		"vue-jest": "3.0.7"
	},
	"resolutions": {
		"@nuxtjs/vuetify/**/sass": "1.35.2"
	}
}
